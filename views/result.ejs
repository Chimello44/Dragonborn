<%- include("header"); -%>

<div class="container-fluid tableMargin">

  <h2>Cross-Connect Records for <%= valueOfData %> </h2>

  <h5 class="text-secondary">I used to be an adventurer like you, then I took an arrow to the knee.</h5>

  <% connection.forEach(function(ckt){ %>

  <table class="table table-dark tableHead btn-secondary">
    <thead>
      <tr>
        <th scope="col">ID</th>
        <th scope="col">Carrier</th>
        <th scope="col">Bandwidth (Gbps)</th>
        <th scope="col">Patch-Panel</th>
        <th scope="col">Port</th>
        <th scope="col">Device</th>
        <th scope="col">Interface</th>
        <th scope="col">AZ</th>
        <th scope="col">Cluster</th>
      </tr>
    </thead>
    <tbody>
      <tr>
      <tr>
        <th scope="row"> <%= ckt._id %> </th>
        <td><%= ckt.serviceprovider %></td>
        <td><%= ckt.bandwidth %></td>
        <td><%= ckt.patchpanel %></td>
        <td><%= ckt.port %></td>
        <td><%= ckt.device %></td>
        <td><%= ckt.interface %></td>
        <td><%= ckt.az %></td>
        <td><%= ckt.cluster %></td>
      </tr>
    </tbody>
  </table>

  <% }); %>
</div>

<%- include("footer"); -%>
